{
  "name": "location",
  "version": "0.0.1",
  "description": "Application 'Location'",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "nodemon": "^2.0.20",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.10"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register  ./node_modules/typeorm/cli.js ",
    "typeorm:migration:generate": "typeorm -- migration:generate src/migrations/*.ts -d ormconfig.json",
    "typeorm:migration:run": "typeorm --  migration:run  -d ormconfig.json",
    "typeorm:migration:create": "npm run typeorm -- migration:create src/migrations/*.ts",
    "typeorm:migration:revert": "typeorm migration:revert"
  }
}
